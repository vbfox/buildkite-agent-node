{
  "name": "buildkite-agent-node",
  "version": "0.0.11-pre.2",
  "description": "Access BuildKite agent commands from NodeJS",
  "main": "dist/index.js",
  "repository": "https://github.com/vbfox/buildkite-agent-node",
  "author": "Julien Roncaglia <julien@roncaglia.fr>",
  "license": "MIT",
  "source": "src/index.ts",
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.3",
    "@types/uuid": "^8.3.0",
    "jest": "^26.4.0",
    "microbundle": "^0.12.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build-lib": "yarn run -s microbundle -f cjs --target node --strict",
    "build": "yarn run -s build-lib",
    "prepublishOnly": "yarn build-lib",
    "preversion": "yarn build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "node-fetch": "^3.1.1",
    "uuid": "^8.0.0",
    "yaml": "^1.9.2"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "directories": {
    "lib": "dist"
  },
  "keywords": [
    "buildkite",
    "agent",
    "ci"
  ]
}
